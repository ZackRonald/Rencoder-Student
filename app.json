{
  "expo": {
    "name": "rencoder",
    "slug": "rencoder",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/splash-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
  "image": "./assets/splash-icon.png",
  "resizeMode": "cover",
  "backgroundColor": "#8968CD"
},
"android": {
  "splash": {
    "image": "./assets/splash-icon.png",
    "resizeMode": "contain",
    "backgroundColor": "#8968CD"
  }
},
"ios": {
  "splash": {
    "image": "./assets/splash-icon.png",
    "resizeMode": "contain",
    "backgroundColor": "#8968CD"
  }
},
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
